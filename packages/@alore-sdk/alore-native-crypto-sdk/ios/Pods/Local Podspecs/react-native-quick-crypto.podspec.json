  "license": "MIT",
  "authors": "Szymon Kapa≈Ça (szymon20000@gmail.com) & Marc Rousavy <me@mrousavy.com> (https://github.com/mrousavy)",
  "platforms": {
    "ios": "12.4",
    "tvos": "12.0",
    "osx": "10.14"
  },
  "source": {
    "git": "https://github.com/mrousavy/react-native-quick-crypto.git",
    "tag": "0.7.0-rc.9"
